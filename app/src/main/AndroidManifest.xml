<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidUI"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.DisplayActivity"
            android:label="@string/string_display">
        </activity>
        <activity
            android:name=".activity.TextViewActivity"
            android:label="@string/string_textview">
        </activity>
        <activity
            android:name=".activity.BuildInfoActivity"
            android:label="@string/string_build_info">
        </activity>
        <activity
            android:name=".activity.LoopTaskActivity"
            android:label="@string/string_loop_task">
        </activity>
        <activity
            android:name=".activity.LoopTask2Activity"
            android:label="@string/string_loop_task2">
        </activity>
        <activity
            android:name=".activity.PreferenceTestActivity"
            android:label="@string/string_preference">
        </activity>
        <activity
            android:name=".activity.ProviderActivity"
            android:label="@string/string_provider">
        </activity>
    </application>

</manifest>